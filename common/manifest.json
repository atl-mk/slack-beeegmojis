  {
    "name": "Slack Beeegmojis",
    "version": "0.5.0",
    "description": "Makes emojis in Slack bigger so you can actually appreciate them.",
    "manifest_version": 2,
    "permissions": ["tabs", "*://*.slack.com/*"],
    "content_scripts": [
        {
            "matches": ["*://*.slack.com/*"],
            "css": ["slack-beeegmojis.css"]
        }
    ]
  }
